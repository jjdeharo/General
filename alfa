


# Genera números enteros al azar normales

aleatn <- function(n=20,media=100,s=10) 
  return(round(rnorm(n,media,s)))

  
	

# Genera números aleatorios enteros	
  aleat <- function(n=10,inicio=1,fin=10)
    return(round(runif(n,inicio,fin)))


anchura <- function(n=75) {
  options(width=n)
  return(1:100)
}
  
  cat("\naleat(n,inicio,fin) para general números aleatorios enteros.")
  cat("\naleatn(n,media,s) para general números aleatorios enteros con distribucion normal.")
  cat("\nanchura(n=95) Cambia la anchura de la consola")


  
  
  cat("\n\n")
